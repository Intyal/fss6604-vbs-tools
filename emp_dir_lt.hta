<body id="tagBody">
    <section class="container" id="stitle">
        <div class="row row-center">
            <div class="column"><h4 class="title" id="FIO">ФИО</h3></div>
        </div>
    </section>
    <section class="container" id="sform">
        <form>
            <div class="row row-center">
                <div class="column">
                    <fieldset>
                        <!-- Кабинет -->
                        <label for="numOffice">Кабинет</label>
                        <input type="text" placeholder="Номер кабинета" id="numOffice" pattern="^[0-9]{1,5}$">
                        <div class="helpmask" forinput="numOffice">x...xxxxx</div>
                        <!-- Внутрнний телефон -->
                        <label for="inTelOffice">Внутрнний телефон</label>
                        <input type="text" placeholder="Номер внутреннего телефона" id="inTelOffice" pattern="^[0-9]{2}-[0-9]{2}$">
                        <div class="helpmask" forinput="inTelOffice">xx-xx</div>
                        <!-- Внешний телефон -->
                        <label for="exTelOffice">Внешний телефон</label>
                        <input type="text" placeholder="Номер внешнего телефона" id="exTelOffice" pattern="^\([0-9]{3}(-|\))([0-9]{0,2}\)|)\s[0-9]{1,3}-[0-9]{2}-[0-9]{2}$">
                        <div class="helpmask" forinput="exTelOffice">(xxx) xxx-xx-xx, (xxx-x) xx-xx-xx, (xxx-xx) x-xx-xx</div>
                        <!-- https://regex101.com/r/5vFxG0/1 -->
                    </fieldset>
                </div>
                <div class="column column-50 column-offset-25">
                    <a class="button" id="OK">Подтвердить</a>
                </div>
            </div>
        </form>
    </section>
    <blockquote>
        <small><p><em id="Status"></em></p></small>
    </blockquote>
</body>
<script language="VBScript">
    Const ADS_PROPERTY_CLEAR = 1
    Const ADS_PROPERTY_UPDATE = 2
    Const ADS_PROPERTY_APPEND = 3
    Const ADS_PROPERTY_DELETE = 4

    ' Размеры окна
    With Window
        .resizeTo 400, tagBody.scrollHeight + 128
        .moveTo (.screen.availWidth - tagBody.offsetWidth) \ 2, (.screen.availHeight - tagBody.offsetHeight) \ 2
    End With

    ' Загрузка данных учетной записи пользователя AD
    Dim objSysInfo
    Set objSysInfo = CreateObject("ADSystemInfo")
    Dim strUserDN
    strUserDN = objSysInfo.UserName

    Dim objLDAPUser
    Set objLDAPUser = GetObject("LDAP://fss.local:389/" & strUserDN)
    objLDAPUser.GetInfo

    ' Заполение полей
    On Error Resume Next
    document.getElementByID("FIO").innerHTML = objLDAPUser.Get("displayName")
    document.getElementByID("numOffice").value = objLDAPUser.Get("physicalDeliveryOfficeName")
    'document.getElementByID("inTelOffice").value = objLDAPUser.Get("otherTelephone")(1)
    'document.getElementByID("exTelOffice").value = objLDAPUser.Get("otherTelephone")(0)
    document.getElementByID("exTelOffice").value = Split(objLDAPUser.Get("telephoneNumber"), ", ")(0)
    document.getElementByID("inTelOffice").value = Split(objLDAPUser.Get("telephoneNumber"), ", ")(1)
    On Error Goto 0
    
    ' Сохранение данных
    Sub OK_OnClick
        If ValidateForm Then
            UpdateProperty document.getElementByID("numOffice").value, "physicalDeliveryOfficeName"
            'Dim arrPhones(1)
            'arrPhones(0) = document.getElementByID("exTelOffice").value
            'arrPhones(1) = document.getElementByID("inTelOffice").value
            'UpdateProperty Join(arrPhones, ";"), "otherTelephone"
            'UpdateProperty Join(arrPhones, ", "), "telephoneNumber"
            UpdateProperty Join(Array(document.getElementByID("exTelOffice").value, document.getElementByID("inTelOffice").value), ", "), "telephoneNumber"

            objLDAPUser.SetInfo

            Window.Close
        Else
           Window.resizeTo 400, tagBody.scrollHeight + 64
        End If
    End Sub

    ' Проверка введенных данных по шаблону
    Function ValidateForm
        Dim strStatus
        strStatus = ""
        ValidateForm = True
        document.getElementByID("Status").innerHTML = strStatus

        Set objRegExp = CreateObject("VBScript.RegExp")
        Set inputList = document.getElementsByTagName("input") 
        For Each Elem In inputList
            If Elem.getAttribute("pattern") <> vbNullString Then
                objRegExp.Pattern = Elem.getAttribute("pattern")
                If objRegExp.Test(Elem.value) Then
                    Set helpList = document.getElementsByClassName("helpmask") 
                    For Each helpElem In helpList
                        If helpElem.getAttribute("forinput") = Elem.id Then
                            helpElem.className = "helpmask masktrue"
                        End If
                    Next
                Else
                    Set helpList = document.getElementsByClassName("helpmask") 
                    For Each helpElem In helpList
                        If helpElem.getAttribute("forinput") = Elem.id Then
                            helpElem.className = "helpmask maskfalse"
                            strStatus = strStatus & "Не верно заполнено поле " & Elem.getAttribute("placeholder") & "<br>"
                        End If
                    Next
                    ValidateForm = False
                End if
            End If
        Next

        document.getElementByID("Status").innerHTML = strStatus
    End Function

    Sub UpdateProperty(strValue, strPropery)
        If strValue <> vbNullString Then
            objLDAPUser.PutEx ADS_PROPERTY_UPDATE, strPropery, Array(CStr(strValue))
        Else
            objLDAPUser.PutEx ADS_PROPERTY_CLEAR, strPropery, vbNullString
        End If
    End Sub
</script>
